<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ShikkhanobishBlazor.ASPNetCore</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />

    <!-- App favicon -->
    <link rel="shortcut icon" href="assets/images/favicon.ico">

    <!-- App css -->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" id="bootstrap-stylesheet" />
    <link href="~/css/icon.min.css" rel="stylesheet" type="text/css" />
    <link href="~/css/app.css" rel="stylesheet" type="text/css" id="app-stylesheet" />


    <link href="~/libs/datatables/dataTables.bootstrap4.css" rel="stylesheet" type="text/css" />
    <link href="~/libs/datatables/responsive.bootstrap4.css" rel="stylesheet" type="text/css" />


</head>
<body>
    
    <main role="main" class="pb-3">
        @RenderBody()
    </main>
  
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/libs/datatables/jquery.dataTables.min.js"></script>
    <script src="~/libs/datatables/dataTables.bootstrap4.min.js"></script>
    <script src="~/libs/morris-js/morris.min.js"></script>
    <script src="~/libs/raphael/raphael.min.js"></script>


    <script src="~/js/pages/dashboard.init.js"></script>
    <script src="~/js/pages/morris.init.js"></script>
    <script src="~/js/pages/chart.min.js"></script>


    <script src="~/js/app.min.js"></script>
    <script src="~/js/dashboard.js"></script>
    <script src="~/js/vendor.min.js"></script>
    <script src="~/js/add.js"></script>
    <script src="~/js/teacher.js"></script>
    <script src="~/js/student.js"></script>
    <script src="~/js/voucher.js"></script>

    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
